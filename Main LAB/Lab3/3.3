#include <stdio.h>

struct student {
    char sex ;
    float gpa ;
};

struct student up(struct student g); 

int main() {
    struct student stu ;
    stu.sex = 'M' ;
    stu.gpa = 2.50 ;
    stu = up(stu) ;
    printf( "%.2f" , stu.gpa ) ;
    return 0 ;
}

struct student up(struct student g){

    g.gpa += g.gpa * 0.1 ; 
    printf( "%f\n" , g.gpa ) ;

    return g ; // คืนค่า สตัรคทั้งหมด

}
