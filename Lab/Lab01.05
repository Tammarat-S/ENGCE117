#include <stdio.h>

int main() {
    int number , even_sum = 0 , odd_sum = 0 ;
    
    if ( scanf( "%d" , &number ) != 1) {
        return 1 ;
    }

    while ( number != 0 ) {
        if ( number % 2 == 0 ) {
            even_sum += number ;
        }else {
            odd_sum += number ;
        }
        if ( scanf( "%d" , &number ) != 1 ) {
            break ; 
        }
    }

    printf( "Even Sum: %d\n" , even_sum ) ;
    printf( "Odd Sum: %d" , odd_sum ) ;
    return 0 ;
}
