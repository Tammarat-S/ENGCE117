#include <stdio.h>

int main() {
    int N , A ;
    int scorespass = 0 , scoresfailed = 0 ;

    if( scanf( "%d" , &N ) != 1 ) {
        return 1 ;
    } 

    int stuscores[N] ;

    for ( A = 0 ; A < N ; A ++ ) {

        if (scanf( "%d" , &stuscores[A] ) != 1 ){
            return 1 ;
        }

        if (stuscores[A] >= 50 ) {
            scorespass += stuscores[A] ;
        }else {
            scoresfailed ++ ;
        }
    }

    printf( "Passing Scores Sum: %d\n" , scorespass ) ;
    printf( "Failing Students Count: %d\n" , scoresfailed ) ;

    return 0 ;
}
